programa
{
	funcao inicio()
	{
		inteiro ano
		escreva("fale um ano aleatorio ai: ")
		leia(ano)
		
		
		se(ano<=0){
			
			ano=2023
		}
		
		se(ano%4==0){
			escreva("Este ano é bissexto \n")
		} senao se(ano%100==0){
			escreva("Este ano é bissexto \n")
		} senao se(ano%400==0){
			escreva("Este ano é bissexto \n")
		} senao{
			escreva("Este ano não é bissexto \n")
		}
	}
}
